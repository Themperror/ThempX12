#target type (Color/Depth)
Type = "Depth"

#Whether to create an SRV for this target
CreateShaderView = false

#target format (See DXGI formats)
Format = "D32_FLOAT"

#(Fixed/Scaling) ([Resolution=Scaling] + [Scale = 1.0] == window/screen resolution)
Resolution = "Fixed"

#Scales target if set to "Scaling" by this amount of the full resolution
Scale = 1.0

#Resolution if set to "Fixed", in pixels
FixedWidth = 1024
FixedHeight = 1024

MultiSampleCount = 1
MultiSampleQuality = 0